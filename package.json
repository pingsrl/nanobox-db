{
	"name": "@pingsrl/nanotools",
	"description": "Connect to Nanobox.io Database",
	"version": "0.0.1",
	"author": "Ping srl @pingsrl",
	"bin": {
		"nanotools": "./bin/run"
	},
	"bugs": "https://github.com/pingsrl/nanotools/issues",
	"dependencies": {
		"@oclif/command": "^1.4.34",
		"@oclif/config": "^1.6.33",
		"@oclif/plugin-help": "^2.0.5",
		"node-yaml": "^3.1.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.15.4",
		"@oclif/test": "^1.1.0",
		"babel-eslint": "^8.2.5",
		"chai": "^4.1.2",
		"eslint": "^4.19.1",
		"eslint-config-oclif": "^1.5.1",
		"eslint-plugin-react": "^7.10.0",
		"fancy-test": "^1.2.0",
		"globby": "^8.0.1",
		"mocha": "^5.2.0",
		"nyc": "^12.0.2"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		"/bin",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"/src"
	],
	"homepage": "https://github.com/pingsrl/nanotools",
	"keywords": [
		"oclif"
	],
	"license": "MIT",
	"main": "src/index.js",
	"oclif": {
		"commands": "./src/commands",
		"bin": "nanotools",
		"plugins": [
			"@oclif/plugin-help"
		]
	},
	"repository": "pingsrl/nanotools",
	"scripts": {
		"postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
		"posttest": "eslint .",
		"prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
		"test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
		"version": "oclif-dev readme && git add README.md"
	}
}
